# A config for Bash.


# - Aliases -
### Navigation
alias ..='cd ..'
alias ...='cd ../..'
alias .3='cd ../../..'
alias .4='cd ../../../..'
alias .5='cd ../../../../..'
alias ~='cd ~'
alias -- -='cd --'

## Navigation - Shortcuts
alias d='cd ~/Documents'
alias dp='cd ~/Dropbox'
alias dl='cd ~/Downloads'
alias dt='cd ~/Desktop'
alias cdw='cd ~/Workspace'
alias cdww='cd ~/Workspace/websites'
